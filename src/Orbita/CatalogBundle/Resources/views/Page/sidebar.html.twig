<section class="section">
    <header>
        <h3>Категории каталога</h3>
    </header>
    <ul>
        {% for cat in categories %}
            <li><a href="{{ path('orbita_catalog_category', { 'id': cat.id }) }}">{{ cat.title }}</a></li>
        {% else %}
            <p>There are no recent categoriess</p>
        {% endfor %}
    </ul>
</section>